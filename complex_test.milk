[preset00]
per_frame_1=q1 = if(above(bass,1.2), 1, -1);
per_frame_2=zoom = 1.0 + 0.02*q1*sin(time*0.5);
per_frame_3=rot = rot + 0.01*q1;
per_frame_4=wave_r = sqr(sin(time));
per_frame_5=wave_g = sqr(cos(time));
per_pixel_1=my_dist = rad;
per_pixel_2=r = if(equal(q1, 1), my_dist, 1.0 - my_dist);
per_pixel_3=g = if(below(my_dist, 0.5), wave_r, wave_g);
per_pixel_4=b = abs(r-g);